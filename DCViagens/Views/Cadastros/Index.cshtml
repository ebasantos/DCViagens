@model IEnumerable<DCViagens.Models.Cadastro>

@{
    ViewBag.Title = "Index";
}

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>

<p>
    <button onclick="/Cadastro/Create" class="btn btn-primary">
        @Html.ActionLink("Create", "Create")
    </button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Beneficiario.EstadoCivil)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Colaborador.Senha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Parceiro.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vendedor.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CIF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Senha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataCadastro)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Beneficiario.EstadoCivil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Colaborador.Senha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parceiro.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vendedor.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CIF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Senha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataCadastro)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
